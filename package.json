{
  "name": "jest-fs-snapshot",
  "version": "0.1.0",
  "description": "Jest matcher for filesystem snapshotting.",
  "author": "Vlad Zhukov (http://github.com/Vlad-Zhukov)",
  "license": "MIT",
  "repository": "Vlad-Zhukov/jest-fs-snapshot",
  "bugs": {
    "url": "https://github.com/Vlad-Zhukov/jest-fs-snapshot/issues"
  },
  "main": "./dist/jest-fs-snapshot.cjs.js",
  "scripts": {
    "build": "rimraf dist/ && cross-env NODE_ENV=production rollup -c",
    "test": "jest",
    "lint": "eslint \"**/*.js\"",
    "prettier": "prettier \"**/*.{js,md}\" --write --loglevel warn",
    "prepublishOnly": "yarn build && yarn lint && prettier \"**/*.{js,md}\" -l && yarn test"
  },
  "dependencies": {
    "asar": "^0.14.3",
    "filenamify": "^2.1.0",
    "iconv-lite": "^0.4.23",
    "jest-diff": "^23.2.0",
    "jest-matcher-utils": "^23.2.0",
    "utf-8-validate": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.54",
    "@babel/preset-env": "7.0.0-beta.54",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "cross-env": "^5.2.0",
    "delay": "^3.0.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "fs-extra": "^7.0.0",
    "jest": "^23.4.1",
    "jest-circus": "^23.4.1",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^4.0.0-beta.7",
    "strip-ansi": "^4.0.0"
  },
  "peerDependencies": {
    "jest": ">=20 <=23"
  },
  "keywords": [
    "test",
    "snapshot",
    "fs-snapshot",
    "jest",
    "jest-snapshot",
    "jest-fs-snapshot"
  ],
  "files": [
    "src",
    "dist"
  ],
  "jest": {
    "testRunner": "jest-circus/runner",
    "roots": [
      "<rootDir>/__tests__/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      "/__fs_snapshots__/"
    ],
    "transform": {
      ".js$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "es5",
    "singleQuote": true,
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "proseWrap": "always"
  }
}
